                <atstart>at(?act,?from)</atstart>
                <overall>connected(?from,?to)</overall>
              </conditions>
              <effects postcond="false">
                <atstart>not(at(?act,?from))</atstart>
                <atend>at(?act,?to)</atend>
              </effects>
              <cost>25.0</cost>
